{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ], "$schema": "https://openapi.vercel.sh/vercel.json",
  "regions": ["sfo1"],
    "routes": [
        {
              { "src": "/(.*)", "dest": "src/main.ts" },

            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}
