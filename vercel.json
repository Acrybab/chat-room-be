{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "functionFailoverRegions": ["iad1", "sfo1"],
  "builds": [
    { "src": "src/main.vercel.ts", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.vercel.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
